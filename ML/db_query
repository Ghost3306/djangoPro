CREATE TABLE `cbot`.`students` (`name` VARCHAR(30) NOT NULL , `age` VARCHAR(10) NOT NULL , `dob` VARCHAR(10) NOT NULL , `phone` VARCHAR(10) NOT NULL , `email` VARCHAR(50) NOT NULL , `occu` VARCHAR(50) NOT NULL , `income` VARCHAR(20) NOT NULL , `pin` VARCHAR(6) NOT NULL , `address` VARCHAR(50) NOT NULL , `gender` VARCHAR(10) NOT NULL , `clss` VARCHAR(10) NOT NULL , `passw` VARCHAR(20) NOT NULL , `pname` VARCHAR(30) NOT NULL ) ENGINE = InnoDB;